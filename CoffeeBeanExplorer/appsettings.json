{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "CoffeeBeanExplorer.Application.Common.Behaviors": "Information",
        "CoffeeBeanExplorer.Application.Common.Behaviors.LoggingBehavior": "Debug",
        "CoffeeBeanExplorer.Application.Common.Services.AttributeReaderService": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithCorrelationId"
    ],
    "Properties": {
      "Application": "CoffeeBeanExplorer",
      "Environment": "Development"
    }
  }
}
